-----------------+                    self.results.append(f"403 on rapid request {i+1}")
-----------------+                    
-----------------+            except Exception as e:
-----------------+                logger.error(f"Error on rapid request {i+1}: {str(e)}")
-----------------+            
-----------------+            time.sleep(0.1)  # Very short delay
-----------------+
