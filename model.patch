-----------------+        # Start 5 concurrent threads
-----------------+        threads = []
-----------------+        for i in range(5):
-----------------+            thread = threading.Thread(target=upload_worker, args=(i,))
-----------------+            threads.append(thread)
-----------------+            thread.start()
-----------------+        
-----------------+        # Wait for all threads to complete
-----------------+        for thread in threads:
-----------------+            thread.join()
